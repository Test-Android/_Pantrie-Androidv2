<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jetts.pantrie" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>

        <activity
            android:name="com.nicodangelo.pantrie.util.ConvertVolume"
            android:label="@string/title_activity_convert_volume" >
        </activity>
        <activity
            android:name="com.nicodangelo.pantrie.util.ConvertMass"
            android:label="@string/title_activity_convert_mass" >
        </activity>
        <activity
            android:name="com.nicodangelo.pantrie.util.Settings"
            android:label="@string/title_activity_settings" >
        </activity>
    </application>

</manifest>
